// Simple GC test - just numbers
main function {
    dekhaw("Starting GC test");
    
    // Create many variables to trigger GC
    purno a = 1;
    purno b = 2;
    purno c = 3;
    purno d = 4;
    purno e = 5;
    
    dekhaw(a);
    dekhaw(b);
    dekhaw(c);
    dekhaw(d);
    dekhaw(e);
    
    // Loop to create more allocations
    purno i = 0;
    jotokkhon (i < 10) {
        purno x = i;
        dekhaw(x);
        i++;
    }
    
    dekhaw("GC test done");
}
