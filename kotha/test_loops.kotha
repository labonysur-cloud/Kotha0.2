main function {
    dhoro i = 0;
    dekhaw("While Loop:");
    jotokkhon (i < 3) {
        dekhaw(i);
        i = i + 1;
    }

    dekhaw("For Loop:");
    cholbe (j theke 1 porjonto 3) {
        dekhaw(j);
    }

    dekhaw("Switch Case:");
    dhoro x = 2;
    paltaw (x) {
        holo 1:
            dekhaw("One");
            ses;
        holo 2:
            dekhaw("Two");
            ses;
        othoba:
            dekhaw("Other");
            ses;
    }
}
