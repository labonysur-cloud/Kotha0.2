main function {
    dekhaw("Logical Operators:");
    dhoro a = 1;
    dhoro b = 0;
    
    jodi (a && b) {
        dekhaw("Both true");
    } othoba {
        dekhaw("Not both true");
    }
    
    jodi (a || b) {
        dekhaw("At least one true");
    }
    
    jodi (!b) {
        dekhaw("b is false");
    }
    
    dekhaw("Increment/Decrement:");
    dhoro x = 5;
    dekhaw(x);
    x++;
    dekhaw(x);
    ++x;
    dekhaw(x);
    x--;
    dekhaw(x);
    --x;
    dekhaw(x);
    
    dekhaw("Compound Assignment:");
    dhoro y = 10;
    dekhaw(y);
    y += 5;
    dekhaw(y);
    y -= 3;
    dekhaw(y);
    y *= 2;
    dekhaw(y);
    y /= 4;
    dekhaw(y);
    y %= 5;
    dekhaw(y);
}
